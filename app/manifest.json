{
    "name": "Github Issues for Chrome",
    "version": "0.0.13",
    "manifest_version": 2,
    "description": "Github issues shows issues for a site you are working on.",
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "default_locale": "en",
    "permissions": [
        "tabs",
        "https://api.github.com/*"
    ],
    "background": {
        "page": "background.html"
    },
    "browser_action": {
        "default_icon": {
            "19": "images/icon-19.png",
            "38": "images/icon-38.png"
        },
        "default_title": "github issues chrome",
        "default_popup": "popup.html"
    },
    "options_page": "options.html"
}
